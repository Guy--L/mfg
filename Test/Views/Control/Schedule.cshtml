@model Test.Models.Plans

<div class="container">
    <br />
    <table id="plans" class="table table-striped table-hover table-condensed">
        <thead>
            <tr>
                <th colspan="8"><h3 class="text-primary">Schedule@(Model.filtered?" for "+Model.Product:"")</h3></th>
            </tr>
            <tr>
                <th>Line</th>
                <th>Scheduled</th>
                <th width="100px;">Product</th>
                <th>Extruder</th>
                <th>Reqd Ft</th>
                <th>Sys</th>                
                <th>Soln</th>
                <th>Comment</th>
            </tr>
        </thead>
        <tbody>
            @Html.DisplayFor(m => m.plans)
        </tbody>
    </table>
</div>

