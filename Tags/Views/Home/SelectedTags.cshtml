@model ChartView

@{
    ViewBag.Title = "Tags from " + Model.List;
    Layout = "~/Views/Shared/_NoFooter.cshtml";
}

@section actions {
@using (var form = Html.Bootstrap().Begin(new Form("GetSpreadSheet", "Home").Type(FormType.Inline).Id("zoomxlsform")))
{ 
    <li>
        <i class="print fa fa-print"> print</i>
            <button class="btn btn-xs btn-success" type="submit"><i class="fa fa-cloud-download"></i>&nbsp;Download viewed Data</button>
    </li>
}
}
<h3>@Model.Title</h3>
<div id="chartId"></div>

@section scripts {
    <script src="~/Scripts/d3/d3.v2.js" type="text/javascript"></script>
    <script src="~/Scripts/tagdata.js" type="text/javascript"></script>
    <script type="text/javascript">
        var tagdata = @Html.Raw(Model.Chart);
        
    </script>
}
